// Because some packages add new users when they install,
// and take the next UID, and we're manually managing UIDs,
// this guy is here to ensure auto-created users take a
// higher UID outside of our user ranges (1000-9000)
{
  "id": "highuid",
  "groups": [ "evertrue" ],
  "uid": 10000,
  "shell": "\/sbin\/nologin",
  "comment": "High UID dummy user",
  "action": "remove"
}
